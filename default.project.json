{
  "name": "simple-inventory",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Packages": {
        "$path": "Packages"
      },
      "Controllers": {
        "$path": "src/Controllers"
      },
      "RoactComponents": {
        "$path": "src/RoactComponents"
      }
    },

    "ServerStorage": {
      "Services": {
        "$path": "src/Services"
      }
    },

    "ServerScriptService": {
      "init": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "ClientScripts": {
          "$path": "src/clientScripts"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}